{
  "name": "building-a-geospatial-app",
  "version": "0.0.1",
  "scripts": {
    "start-local": "webpack-dev-server --env.local --progress --hot --port 3030 --open",
    "start:client": "webpack-dev-server --progress --hot --port 3030 --open",
    "start:server": "cd backend && nodemon server.js --exec babel-node --presets es2015,stage-0",
    "start:dev": "concurrently \"yarn start:server\" \"yarn start:client\"",
    "build": "NODE_ENV=production webpack --env.prod=true"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.3",
    "concurrently": "^3.5.1",
    "deck.gl": "^4.1.0",
    "express": "^4.16.3",
    "immutable": "^3.8.1",
    "luma.gl": "^4.0.1",
    "mongoose": "^5.1.5",
    "morgan": "^1.9.0",
    "nodemon": "^1.17.5",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-map-gl": "^3.0.4",
    "react-vis": "^1.7.2"
  },
  "devDependencies": {
    "buble-loader": "^0.4.0",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  },
  "proxy" : "http://localhost:3001"
}
